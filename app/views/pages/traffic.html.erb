<div class="container-fluid">
	<div class='row'>
		<div class='col-md-4 col-xs-6'>
			<% @impressions.each do |impression| %>
				<h3><%= impression.ip_address %></h3>
				<p>Referrer : <%= impression.referrer %></p>
				<p>User : <%= impression.user.name %></p>
				<p>Email Address : <%= impression.user.email %></p>
			<% end %>
		</div>
	</div>
</div>